generator client {
  provider = "prisma-client-js"
}

// datasource db {
//   provider     = "mysql"
//   url          = env("DATABASE_URL")
//   relationMode = "prisma"
// }

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Client {
  id    String @id @default(uuid())
  name  String
  phone String @unique

  created_at DateTime @default(now())

  @@map("clients")
}
